{
    "name": "cloudest-co/laravel-eloquent-email-log",
    "description": "",
    "keywords": [
        "cloudest",
        "laravel-eloquent-email-log"
    ],
    "homepage": "https://github.com/cloudest-co/laravel-eloquent-email-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Reid",
            "email": "chris@cloudest.co.uk",
            "homepage": "https://cloudest.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Cloudest\\LaravelEloquentEmailLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cloudest\\LaravelEloquentEmailLog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cloudest\\LaravelEloquentEmailLog\\LaravelEloquentEmailLogServiceProvider"
            ]
        }
    }
}
